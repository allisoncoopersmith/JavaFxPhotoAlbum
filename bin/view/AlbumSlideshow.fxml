<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>  
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="411.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text layoutX="167.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Album Slideshow">
         <font>
            <Font size="33.0" />
         </font>
      </Text>
      <ImageView fx:id="SlideshowImageView" fitHeight="265.0" fitWidth="366.0" layoutX="117.0" layoutY="73.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="PreviousImageBtn" layoutX="61.0" layoutY="357.0" mnemonicParsing="false" onAction="#PreviousImageBtnHandler" text="Previous Image" />
      <Button fx:id="NextImageBtn" layoutX="439.0" layoutY="357.0" mnemonicParsing="false" onAction="#NextImageBtnHandler" text="Next Image" />
      <Button fx:id="returnToAlbumBtn" layoutX="462.0" layoutY="11.0" mnemonicParsing="false" onAction="#returnToAlbumBtnHandler" text="Return to Album" />
   </children>
</AnchorPane>
