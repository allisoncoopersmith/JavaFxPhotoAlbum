<AnchorPane prefHeight="366.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text layoutX="167.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="User Profile">
         <font>
            <Font size="48.0" />
         </font>
      </Text>
      <ListView fx:id="AlbumListView" layoutX="236.0" layoutY="65.0" prefHeight="169.0" prefWidth="266.0" />
      <Button fx:id="ViewAlbumButton" layoutX="236.0" layoutY="249.0" mnemonicParsing="false" onAction="#ViewAlbumButtonHandler" text="View Album" />
      <Button fx:id="RenameAlbumButton" layoutX="336.0" layoutY="325.0" mnemonicParsing="false" onAction="#RenameAlbumButtonHandler" text="Rename Album" />
      <Button fx:id="DeleteAlbumButton" layoutX="420.0" layoutY="249.0" mnemonicParsing="false" onAction="#DeleteAlbumButtonHandler" text="Delete Album" />
      <Button fx:id="AddAlbumButtonHandler" layoutX="113.0" layoutY="325.0" mnemonicParsing="false" onAction="#AddAlbumButtonHandler" text="Add Album" />
      <Button fx:id="SearchButton" layoutX="60.0" layoutY="249.0" mnemonicParsing="false" onAction="#SearchButtonHandler" text="Search" />
      <Button fx:id="ExitButton" layoutX="521.0" layoutY="19.0" mnemonicParsing="false" onAction="#ExitButtonHandler" prefHeight="40.0" prefWidth="61.0">
         <graphic>
            <ImageView fitHeight="36.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../exit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <TextField fx:id="addAlbumTF" layoutX="84.0" layoutY="290.0" />
      <TextField fx:id="renameAlbumTF" layoutX="300.0" layoutY="290.0" />
      <ImageView fx:id="imageView" fitHeight="134.0" fitWidth="129.0" layoutX="70.0" layoutY="89.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
